                                <div class="h-90 bg-cover cursor-pointer position-relative thumb_worktop" style="background-image: url({% if categories[object]['imageThumbnail']['content'] is defined %}{{ categories[object]['imageThumbnail']['content']|replace({' ':'%20'}) }}{% endif %})" data-name="">
                                    <div class="thumb_worktop__more position-absolute d-flex bottom-0 h-90">
                                        <p class="p-2 text-center small text-white m-auto">
                                            {{ categories[object]['shortDesc']['content'] }}
                                        </p>
                                    </div>
                                </div> 
                            </a>
                            <p class="text-center pt-1 pb-3">{{ categories[object]['shortDesc']['content'] }}</p>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
</div>


