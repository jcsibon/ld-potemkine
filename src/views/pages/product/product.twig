<script>
    $('.fp__display_thumbnails_container2>div').click(function() {
        $('.fp__display_thumbnails_container2>div').removeClass('fp__selected');
        $(this).addClass('fp__selected');
        $('#fullImageImageContainer>img').attr('src', $(this).find('img').attr('data-image-md-url'));
        $('#popupPhotoZoom img').attr('src', $(this).find('img').attr('data-image-hd-url'));
        if ($(this).find('img').attr('data-embed-url')) {
            $('#fullImageVideoContainer iframe').attr('src', $(this).find('img').attr('data-embed-url'));
            $('#fullImageImageContainer').hide(); 
            $('#fullImageVideoContainer').show(); 
        } else {
            $('#fullImageImageContainer').show(); 
            $('#fullImageVideoContainer').hide();   
        }
    });

    $('.fp__color').click(function() {
        $('.fp__color').removeClass('fp__selected');
        $('.fp__display_thumbnails_container2>div').removeClass('fp__selected');
        $(this).addClass('fp__selected');
        $('#fullImageImageContainer>img').attr('src', $(this).attr('data-image-md-url'));
        $('#popupPhotoZoom img').attr('src', $(this).attr('data-image-hd-url'));
        if (!$('#fullImageImageContainer>img').is(":visible")) $('#popupPhotoZoom').modal();
    });

    $("#fp__quantity").keypress(function (e) {
        if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) return false;
    });

    $("#qtyLeftBtn").click(function() {
        if (!$("#qtyLeftBtn").hasClass('text-muted')) $("#fp__quantity").val(parseInt($("#fp__quantity").val())-1);
        if (parseInt($("#fp__quantity").val())<=1) $("#qtyLeftBtn").addClass('text-muted');
        refresh_packing_qty();
    });

    $("#qtyRightBtn").click(function() {
        $("#qtyLeftBtn").removeClass('text-muted');
        if (!parseInt($("#fp__quantity").val())) $("#fp__quantity").val('0');  
        $("#fp__quantity").val(parseInt($("#fp__quantity").val())+1);
        refresh_packing_qty();
    });

    $("#fp__area").keypress(function (e) {
        if (e.which != 8 && e.which != 0 && (e.which < 46 || e.which > 57)) return false;
    });

    $("#fp__area").focusout(function() {
        refresh_packing(); 
    });

    $("#fp__area").keypress(function(e){
        if(e.which == 13) refresh_packing();
    });

    $("#fp__quantity").focusout(function() {
        refresh_packing_qty(); 
    });

    $("#fp__quantity").keypress(function(e){
        if(e.which == 13) refresh_packing_qty();
    });

    function refresh_packing() {
        $("#fp__quantity").val(Math.ceil(parseInt($("#fp__area").val())/$("#fp__area").attr("data-area-value")));
        $("#fp__area").val(Math.round(parseInt($("#fp__quantity").val())*$("#fp__area").attr("data-area-value")*100)/100);
    }

    function refresh_packing_qty() {
        $("#fp__area").val(Math.round(parseInt($("#fp__quantity").val())*$("#fp__area").attr("data-area-value")*100)/100);
    }

    function checkReviewCarousel() {  //Pour ne pas avoir un carousel infini
        $('#avis_clients a[class^="carousel-control"]').css('display','flex');
        $('.viewMoreMobile').hide();
        if($('#avis_clients .carousel-inner .carousel-item:first').hasClass('active')) {
            $('#avis_clients a.carousel-control-prev').hide();
        } else if($('#avis_clients .carousel-inner .carousel-item:last').hasClass('active')) {
            $('#avis_clients a.carousel-control-next').hide();
            $('#avis_clients .viewMoreMobile').css('display','flex');
        }
    }

    $('#carouselReviews').on('slid.bs.carousel', function() {
        checkReviewCarousel();
    });

    $( document ).ready(function() {
        checkReviewCarousel();
    });


    //Carousel vertical miniatures - Start
    var container1 = $('.fp__display_thumbnails_container1');
    var container2 = $('.fp__display_thumbnails_container2');

    $('.fp__next_thumbnails').on('click', function(e){
        var $link = $(e.target);
        e.preventDefault();
        if(!$link.data('lockedAt') || +new Date() - $link.data('lockedAt') > 300) {
            var shift = parseInt(container1.css('height'));
            var topShift = parseInt(container2.css('margin-top')) - shift;
            container2.attr('style','margin-top:' + topShift + 'px !important;');
            checkThumbnailsCarousel();
        }
        $link.data('lockedAt', +new Date());
    }); 

    $('.fp__prev_thumbnails').on('click', function(e){
        var $link = $(e.target);
        e.preventDefault();
        if(!$link.data('lockedAt') || +new Date() - $link.data('lockedAt') > 300) {
            var shift = parseInt(container1.css('height'));
            var topShift = parseInt(container2.css('margin-top')) + shift;
            container2.attr('style','margin-top:' + topShift + 'px !important; ');
            checkThumbnailsCarousel();
        }
        $link.data('lockedAt', +new Date());
    });  

    function checkThumbnailsCarousel() {
        setTimeout(function(){ 
            if (parseInt(container2.css('margin-top')) >= 0) { 
                $('.fp__prev_thumbnails').attr('style','display:none !important');
            } else {
                $('.fp__prev_thumbnails').attr('style','');
            }
            if ((container2.offset().top+container2.height())<(container1.offset().top+container1.height())) {
                $('.fp__next_thumbnails').attr('style','display:none !important');
            } else {
                $('.fp__next_thumbnails').attr('style','');
            }
        }, 500);
    }

    function resizeThumbnailsCarousel() {
        container1.css('height', ($('.fp__display_thumbnails img').height() * 4 + 42) + 'px');
    }

    $( document ).ready(function() {
        resizeThumbnailsCarousel();
        checkThumbnailsCarousel();
    });

    $( window ).resize(function() {
        resizeThumbnailsCarousel();
    });
    //Carousel vertical miniatures - End

</script>

